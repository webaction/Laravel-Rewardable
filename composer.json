{
    "name": "faustbrian/laravel-rewardable",
    "description": "Credits, Badges and Ranks for Eloquent Models for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-rewardable", "Laravel Rewardable", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.0",
        "illuminate/database": "5.3.* || 5.4.* || 5.5.*",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.*",
        "faustbrian/eloquent-models": "^1.0",
        "faustbrian/eloquent-presenter": "^1.0",
        "nesbot/carbon": ">=1.21.0",
        "spatie/laravel-medialibrary": "^3.1",
        "spatie/laravel-sluggable": "^1.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Rewardable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Rewardable\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Rewardable\\RewardableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
